{
  "name": "idavoll/core-module",
  "type": "asgard-module",
  "description": "The core module for AsgardCMS. This is required for every install.",
  "keywords": [
    "asgardcms",
    "core"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Platform/issues",
    "source": "https://github.com/AsgardCms/Core"
  },
  "require": {
    "php": ">=8.3",
    "composer/installers": "*",
    "nwidart/laravel-modules": "*",
    "laravelcollective/html": "*",
    "laracasts/presenter": "*",
    "astrotomic/laravel-translatable": "*",
    "mcamara/laravel-localization": "*",
    "imagina/stylist": "*",
    "maatwebsite/laravel-sidebar": "*",
    "yajra/laravel-datatables-oracle": "*",
    "tightenco/ziggy": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "orchestra/testbench": "*",
    "mockery/mockery": "*",
    "friendsofphp/php-cs-fixer": "*"
  },
  "suggest": {
    "asgardcms/notification-module": "Allows notifications to be sent to the user. Optionally real-time notifications."
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Core\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
